        <div id="content">
            <div id="content-inner">
                <div id="alpha">
                    <div id="alpha-inner">

                        <h1 id="page-title" class="archive-title">
                            <% if(page.tag){ %>
                                <%= page.tag%>
                                    <%}else{%>文章存档
                                        <%}%>
                        </h1>
                        <div class="module-categories module">
                            <h2 class="module-header">一共有<%= page.posts.length %>篇文章</h2>
                            <div class="module-content">
                                <ul class="module-list">
                                    <% page.posts.each(function(post){ %>
                                        <li class="module-list-item">2018.03.05：
                                            <a href="<%- url_for(post.path)%>">
                                                <%= post.title%>
                                            </a>
                                        </li>
                                        <% }) %>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="beta">
                    <div class="module-categories module">
                        <h2 class="module-header">关于我</h2>
                        <div class="module-content">
                            <ul class="module-list">
                                <p class="module-list-item">
                                    <a href="/">
                                        <img src="<%- url_for(theme.img) %>/Heads.jpg" height="106" alt="个人照片" />
                                    </a>
                                </p>
                                <li class="module-list-item">
                                    <a href="/about.html">微博</a>
                                    <a href="2015/02/turing-interview.html" target="_blank">github</a>
                                </li>
                                <li class="module-list-item">
                                    吾将上下而求索...
                                </li>
                                <li class="module-list-item">
                                    <a href="/about">更多</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div id="beta-inner">
                        <div class="module-categories module">
                            <h2 class="module-header">标签</h2>
                            <div class="module-content">
                                <ul class="module-list">
                                    <% site.tags.map(function(tag){  %>
                                        <li class="module-list-item">
                                            <a href="/tags/<%= tag.name %>" title="散文">
                                                <%= tag.name %>
                                            </a>(
                                            <%= tag.length %>)
                                        </li>
                                        <% }) %>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
