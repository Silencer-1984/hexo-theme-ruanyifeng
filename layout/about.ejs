<script>
    document.getElementsByTagName("body")[0].className= "";
    document.getElementsByTagName("body")[0].classList.add("mt-entry-archive")
    document.getElementsByTagName("body")[0].classList.add("one-column")
    if (/mobile/i.test(navigator.userAgent) || /android/i.test(navigator.userAgent)) {document.body.classList.add('mobile')}else{
        document.body.classList.remove('mobile')
    };
</script>
<div id="container">
    <div id="container-inner">
        <div id="header">
            <div id="header-inner">
                <div id="header-content">
                    <div id="header-description">
                        <a href="/" accesskey="1">博客首页</a>
                    </div>
                </div>
            </div>
        </div>
        <div id="content">
            <div id="content-inner">
                <div id="alpha">
                    <div id="alpha-inner">
                        <div id="page-1750" class="page-asset asset">
                            <div class="asset-header">
                                <h1 id="page-title" class="asset-name">About Me</h1>
                            </div>
                            <div class="asset-content" id="main-content">
                                <%- page.content %>
                            </div>

                            <div class="asset-footer">
                                <ul>
                                    <li>最后修改时间： <%= page.date.format(config.date_format)%> <%= page.date.format(config.time_format)%></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="beta">
                    <div id="beta-inner">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>